    "stylelint": "^15.10.3",
    "stylelint-webpack-plugin": "^4.1.1",
    "svg-sprite": "^1.5.4",
    "svg-spritemap-webpack-plugin": "^4.5.0",
    "svgxuse": "^1.2.6",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.9.0"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "webpack-cli": "^5.1.4"
  }
