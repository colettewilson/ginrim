{
  "name": "frontend-barebones",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@svn.builtbybuffalo.com:buffalo/fe-barebones.git"
  },
  "scripts": {
    "clean": "./craft clear-caches/all && ./craft cache/flush-all",
    "clean:dist": "rm -rf ./web/dist/*",
    "clean:all": "yarn run clean:dist && yarn run clean",
    "watch": "yarn run clean:dist && NODE_ENV=development webpack serve --mode development --config webpack.dev.js",
    "develop": "php -S localhost:8080 -t public",
    "build": "NODE_ENV=production && webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@babel/core": "^7.22.20",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "coffee-script": "^1.12.7",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "del": "^7.1.0",
    "exports-loader": "^4.0.0",
    "featherlight": "^1.7.14",
    "flickity": "^3.0.0",
    "fontfaceobserver": "^2.3.0",
    "foundation-sites": "^6.8.1",
    "fslightbox": "^3.4.1",
    "glob": "^10.3.4",
    "hammerjs": "^2.0.8",
    "imports-loader": "^4.0.1",
    "jquery": "^3.7.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass-glob-importer": "^5.3.3",
    "normalize.css": "^8.0.1",
    "postcss-easings": "^4.0.0",
